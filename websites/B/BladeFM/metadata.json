	"settings": [
		{
			"id": "timestamp",
			"title": "Show Timestamp",
			"icon": "fas fa-history",
			"value": true
		},
		{
			"id": "details",
			"title": "Change Line #1 (Song Details)",
			"icon": "fas fa-music",
			"value": "Streaming to %listeners% listeners",
			"placeholder": "%artist%, %listeners%, %songText%, %title%"
		},
		{
			"id": "state",
			"title": "Change Line #2 (Presenter Details)",
			"icon": "fas fa-microphone",
